    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "sass-loader": "7.1.0",
    "sass-resources-loader": "2.0.1",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "thread-loader": "2.1.2",
    "url-loader": "2.0.1",
    "webpack": "4.35.2",
    "webpack-bundle-analyzer": "3.3.2",
