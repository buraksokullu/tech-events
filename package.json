{
  "name": "TrivagoChallange",
  "version": "1.0.0",
  "description": "Trivago Challange",
  "main": "app/index.js",
  "author": "Burak Sokullu",
  "license": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "trivago",
    "challenge"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=Development webpack-dev-server --mode development --display-error-details --progress --config webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=Production webpack-dev-server --mode development --display-error-details --progress --https --config webpack.dev.js",
    "build": "webpack-cli -p --progress --display-error-details --verbose --mode production --config webpack.prod.js",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint app/**/*.js --fix",
    "prettier": "prettier app/**/*.js app/**/*.scss --write --config .prettierrc.js",
    "precommit": "lint-staged",
    "svg:convert": "svgr -d ./app/components/svg ./app/assets/images/svg",
    "svg:index": "create-index ./app/components/svg",
    "svgToReact": "npm run svg:convert && npm run svg:index",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@hot-loader/react-dom": "16.8.6",
    "axios": "0.19.0",
    "bootstrap": "^4.4.1",
    "classnames": "2.2.6",
    "dayjs": "1.8.14",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "history": "4.9.0",
    "immutable": "4.0.0-rc.12",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.12.1",
    "react-redux": "7.1.0",
    "react-redux-toastr": "^7.6.4",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1",
    "redux-form": "^8.3.0",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@svgr/cli": "4.3.1",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "connect-history-api-fallback": "1.6.0",
    "create-index": "2.4.0",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "extract-css-chunks-webpack-plugin": "4.5.4",
    "file-loader": "4.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.0",
    "jest": "24.9.0",
